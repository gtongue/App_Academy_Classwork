<h1>Sign Up</h1>

<form id='user' action="<%=users_url%>" method="post">
  <%= auth_token %>
  <label>Username
    <input type="text" name="user[username]">
  </label>

  <label>Password
    <input type="text" name="user[password]">
  </label>

  <input type="submit" name="" value="Submit">
</form>
